import java.util.Scanner;
public class tugas1
{
  public static void main (String []args)
  {
    int i,x;
    x=1;
    Scanner s=new Scanner(System.in);
    System.out.println(" ");
    System.out.printf("Input Angka:");
    int N=s.nextInt();
    for (i=1 ; i<=N ; i++)
		{
			System.out.print(+i) ;
			if(i<N)
			{
				System.out.print(" x ");
			}
			else
			{
				System.out.print(" = ");
			}
			x=x*i;
		}
		System.out.print(+x) ;
	}
}
    
  }
  
}
